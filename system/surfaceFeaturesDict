{
    format      ascii;
    class       dictionary;
    object      surfaceFeaturesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaces (
    "stationaryShaft.stl"
    "shaftRotational.stl"
    "leftPort.stl"
    "rightPort.stl"
    "discSide.stl"
    "bottomPort.stl"
    "topPort.stl"
    "frontCounterPlate.stl"
    "discFront.stl"
    "backCounterPlate.stl"
    "AMI.stl"
    "sideWall.stl"
    "discBack.stl"
    );

includedAngle       150;

subsetFeatures
{
    nonManifoldEdges       yes;

    openEdges       yes;
}

